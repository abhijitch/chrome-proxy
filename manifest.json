{
  "manifest_version":2,
  "name": "Chrome Proxy",
  "version": "1.0",
  "description": "proxy only in-scope domains",
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/test-48.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "proxy"
  ],
  "icons": {
    "48": "icons/test-48.png"
  }
}

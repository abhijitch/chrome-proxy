{
  "manifest_version":2,
  "name": "Chrome Proxy",
  "version": "1.0",
  "description": "Proxy a specific tab through BurpSuite",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/test-48.png"
  },
  "permissions": [
    "proxy",
    "tabs"
  ],
  "icons": {
    "48": "icons/test-48.png"
  },
  "commands": {
    "toggle-proxy": {
      "suggested_key": {
        "default": "Ctrl+Shift+I",
        "mac": "Command+Shift+I"
      },
      "description": "Toggle proxying of tab."
    }
  }
}
